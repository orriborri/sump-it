{
  "name": "postgres-kysely",
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "lint:lenient": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "generate:db-types": "kysely-codegen --env-file .env.local --out-file ./app/lib/db.d.ts --log-level debug",
    "generate:models": "tsx ./generate-models.ts",
    "migrate": "node --loader ts-node/esm ./migrate.ts",
    "docker:build": "docker build -t sump-it .",
    "docker:run": "docker run -p 3000:3000 sump-it",
    "docker:compose": "docker-compose up --build",
    "docker:compose:down": "docker-compose down",
    "ci": "run-s type-check lint format:check test",
    "setup": "run-s migrate generate:db-types generate:models",
    "docker:full": "run-s docker:build docker:run",
    "check-all": "run-p type-check lint format:check",
    "check-strict": "run-s type-check lint format:check",
    "pre-commit": "run-s type-check lint format:check",
    "generate-all": "npm-run-all migrate generate:*",
    "clean": "rm -rf .next dist node_modules/.cache",
    "reset-db": "pnpm run migrate && pnpm run generate-models",
    "dev:clean": "pnpm run clean && pnpm run dev",
    "analyze": "ANALYZE=true pnpm run build",
    "dev:telemetry": "OTEL_SERVICE_NAME=sump-it-dev pnpm run dev",
    "test:health": "curl -s http://localhost:3000/api/health | jq",
    "test:metrics": "curl -s http://localhost:9090/metrics"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@eslint/eslintrc": "^3.3.1",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@mui/material-nextjs": "^7.1.1",
    "@mui/mcp": "^0.1.0",
    "@mui/x-data-grid": "^8.11.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.64.1",
    "@opentelemetry/exporter-jaeger": "^2.1.0",
    "@opentelemetry/exporter-prometheus": "^0.205.0",
    "@opentelemetry/instrumentation-express": "^0.54.0",
    "@opentelemetry/instrumentation-http": "^0.205.0",
    "@opentelemetry/instrumentation-pg": "^0.58.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@types/ms": "^2.1.0",
    "@types/pg": "^8.15.4",
    "@types/qrcode": "^1.5.5",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "@vercel/postgres": "0.10.0",
    "@vercel/postgres-kysely": "0.10.0",
    "autoprefixer": "10.4.21",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "knex": "^3.1.0",
    "kysely": "^0.28.2",
    "ms": "^2.1.3",
    "mysql2": "^3.14.1",
    "next": "15.5.4",
    "pg": "^8.16.2",
    "postcss": "8.5.6",
    "qrcode": "^1.5.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.58.1",
    "react-router-dom": "^7.6.2",
    "save": "^2.9.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@next/eslint-plugin-next": "^15.3.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/mui-datatables": "^4.3.12",
    "@types/node": "24.6.2",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "better-sqlite3": "^12.2.0",
    "eslint": "9.37.0",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsdom": "^27.0.0",
    "kysely-codegen": "^0.19.0",
    "kysely-ctl": "^0.19.0",
    "npm-run-all2": "^8.0.0",
    "prettier": "^3.6.2",
    "tailwindcss": "4.1.14",
    "ts-morph": "^27.0.0",
    "ts-node": "10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
